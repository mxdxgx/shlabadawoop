{
  "name": "shlabadawoop",
  "version": "1.5.1",
  "description": "shlabadawoop node.js api written in typescript for sheer, unlimited joy",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "prebuild": "npm run clean",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "pretest": "npm run build",
    "test": "run-script-os",
    "test:win32": ".\\node_modules\\.bin\\nyc .\\node_modules\\.bin\\mocha --timeouts=15000 --require ts-node/register .\\src\\**\\*.spec.ts",
    "test:darwin:linux": "./node_modules/.bin/nyc ./node_modules/.bin/mocha --timeouts=15000 --require ts-node/register ./src/**/*.spec.ts",
    "start": "node dist/src/server/server.js",
    "start:dev": "run-script-os",
    "start:dev:win32": ".\\node_modules\\.bin\\nodemon",
    "start:dev:darwin:linux": "./node_modules/.bin/nodemon",
    "prepare": "husky install"
  },
  "author": "Maxime G",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/chai": "^4.3.6",
    "@types/config": "3.3.1",
    "@types/express": "^4.17.18",
    "@types/mocha": "^10.0.2",
    "@types/nock": "^10.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "chai": "^4.3.10",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nock": "^13.3.3",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "config": "^3.3.9",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "js-yaml": "^4.1.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "typescript": "^5.2.2",
    "winston": "^3.10.0"
  }
}